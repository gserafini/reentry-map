{
  "private": true,
  "scripts": {
    "tailwind:build": "node node_modules/tailwindcss/lib/cli.js -i ./styles/tailwind-input.css -o ./styles/tailwind.css --minify",
    "tailwind:watch": "node node_modules/tailwindcss/lib/cli.js -i ./styles/tailwind-input.css -o ./styles/tailwind.css --watch",
    "dev": "(lsof -ti:3003 | xargs kill -9 2>/dev/null || true) && rm -rf .next && concurrently \"npm:tailwind:watch\" \"node node_modules/next/dist/bin/next dev --turbopack --port 3003\"",
    "dev:test": "(lsof -ti:3004 | xargs kill -9 2>/dev/null || true) && rm -rf .next && concurrently \"npm:tailwind:watch\" \"node node_modules/next/dist/bin/next dev --turbopack --port 3004\"",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "SKIP_ENV_VALIDATION=true vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "SKIP_ENV_VALIDATION=true vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:a11y": "playwright test e2e/accessibility.spec.ts",
    "test:a11y:ui": "playwright test e2e/accessibility.spec.ts --ui",
    "dev:check": "bash scripts/check-dev-compile.sh",
    "console:check": "node scripts/check-console.mjs / && node scripts/check-console.mjs /resources && node scripts/check-console.mjs /admin",
    "quality": "npm run lint && npx tsc --noEmit && npm run test:run && npm run build && npm run dev:check && npm run console:check",
    "quality:full": "npm run quality && npm run test:e2e",
    "quality:a11y": "npm run lint && npx tsc --noEmit && npm run test:a11y",
    "screenshots": "bash scripts/screenshot.sh",
    "geoip:update": "node node_modules/geoip-lite/scripts/updatedb.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@googlemaps/js-api-loader": "^2.0.1",
    "@googlemaps/markerclusterer": "^2.6.2",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.2",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "latest",
    "@t3-oss/env-nextjs": "^0.13.8",
    "cheerio": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "geoip-lite": "^1.4.10",
    "lucide-react": "^0.511.0",
    "next": "latest",
    "next-themes": "^0.4.6",
    "openai": "^6.6.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.0",
    "use-debounce": "^10.0.6",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^16.0.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/crypto-js": "^4.2.2",
    "@types/geoip-lite": "^1.4.4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.1",
    "@vitest/ui": "^4.0.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "lint-staged": "^16.2.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,css,html,yml,yaml}": [
      "prettier --write"
    ]
  }
}
